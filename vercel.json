{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "MONGODB_URI": "@MONGODB_URI",
    "API_KEY": "@API_KEY",
    "CORS_ORIGIN": "@CORS_ORIGIN",
    "PORT": "3000"
  }
}